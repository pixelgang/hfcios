<?xml version="1.0" encoding="UTF-8" ?>
<widget id="com.digitalharm.house" version="1.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">

	<name>House for Cash</name>
	<description>House submital app</description>
	<author href="http://www.digitalharmony.agency" email="support@digitalharmony.agency">Digital Harmony Agency</author>

	
	<icon src="www/icon-76@2x.png" gap:platform="ios" width="152" height="152" />
	<icon src="www/icon-76.png" gap:platform="ios" width="76" height="76" />
	
	<icon src="www/Icon-App-40x40@3x.png" gap:platform="ios" width="120" height="120" />
	<icon src="www/Icon-App-60x60@2x.png" gap:platform="ios" width="120" height="120" />
	
	<icon src="www/Icon-App-76x76@1x.png" gap:platform="ios" width="76" height="76" />
	<icon src="www/Icon-App-76x76@2x.png" gap:platform="ios" width="152" height="152" />

	<preference name="phonegap-version" value="cli-8.0.0" />
	<preference name="android-targetSdkVersion" value="26" />
	

	<preference name="orientation" value="portrait" />
	<preference name="fullscreen" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="AllowInlineMediaPlayback" value="true" />

	<preference name="deployment-target" value="9.0" />
	<preference name="android-minSdkVersion" value="21" />
	<access origin="*" />

	<plugin name="cordova-plugin-inappbrowser" />
	<plugin name="cordova-plugin-whitelist" />


	<engine name="ios" />
	<engine name="android" />
</widget>
